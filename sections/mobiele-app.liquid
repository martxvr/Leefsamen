{% stylesheet %}

.wrapper-app {
    position: relative;
    display: flex;
    justify-content: center;
    align-items: stretch; /* Veranderd van "align-items: left" */
    flex-direction: column;
    text-align: left;
    margin: auto;
    max-width: 1100px;
    /* Verwijder de hoogte-instellingen */
    background-color: #fff;
}


.mobile-app {
    position: absolute ;
    bottom: -98px;
    right: 20px;
    transform: scale(0.7); 
    width: 470px;
    height: 651px;
}

.usp-section-app {
    margin-top: 20px;
    margin-bottom: 40px;
}

.usp-app {
    display: flex;
    flex-direction: row;
    gap: 10px;
}

.container-app {
    width: 600px;
}

.container-app h1 {
    font-size: 50px;
    font-weight: 700;
}


.usp-app svg{
    width: 32px;
    height: 32px;
    height: auto;
}

.usp-app p {
  margin: 5px;
  color: #4f4f4f;
}

.cta-app {
    display: flex;
    flex-direction: row;
    height: auto;
    margin-top: -30px;
    gap: 20px;
}

.cta-app svg {
    width: 120px;
    height: 120px;
    padding-top: 7px;
}

  
    #app-store {
       width: 137px;
      height: 127px;
      padding: 0;
    }

  @media only screen and (max-width: 600px) {
  .container-app {
    max-width: 600px;
    width: auto;
    margin: 0;
    margin: 32px;
  }

  .cta-app svg {
    padding: 0;
  }

  .cta-app {
    display: flex; 
    flex-direction: row; 
    justify-content: center; 
    align-items: center;
    width: auto; 
    height: auto; 
    margin-top: 0;
  }   

  .container-app h1 {
    font-size: 40px;
    text-align: center;
  }  

  .container-app p {
    margin: 10px;
    
  } 

  .usp-section-app {
     margin-bottom: 0px;
  }  
}
   @media only screen and (max-width: 1120px) {
        .mobile-app {
          display: none;
        } 
  }
  
